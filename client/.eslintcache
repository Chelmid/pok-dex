fect(() => {\r\n        dispatch({\r\n            type: 'STATUS_ONE_POKEMON',\r\n            display: true,\r\n            pokedex: false\r\n        })\r\n    }, [dispatch])\r\n\r\n    const handleSubmitConnect = (e) => {\r\n        e.preventDefault()\r\n        if (email.length >= 4 && pwd.length >= 5) {\r\n            axios.post('/login', {\r\n                email: email,\r\n                password: pwd\r\n            }).then(res => {\r\n                /*history.push('/login')*/\r\n                console.log(res)\r\n                axios.get(res.request.responseURL)\r\n                    .then(response => {\r\n                        let str = response.request.responseURL\r\n                        if (str.includes(\"/login/success/\") == true || res.data.message === 'connecter') {\r\n                            dispatch({\r\n                                type: 'CONNECT',\r\n                                connection: true\r\n                            })\r\n                            setCookie('connect', true, { path: '/pokemon/list' })\r\n                            setCookie('email', email, { path: '/pokemon/list' })\r\n                            setCookie('connect', true, { path: '/' })\r\n                            setCookie('email', email, { path: '/' })\r\n                            history.push('/pokemon/list')\r\n                        } else {\r\n                            setMsg(\"erreur mot de passe ou d'adresse mail\")\r\n                        }\r\n                    })\r\n            }).catch(error => setMsg('server erreur'))\r\n            setMessage('')\r\n        } else {\r\n            if (pwd.length <= 5) {\r\n                setMessage('Veuillez remplir avec plus de 5 charateres')\r\n                setBorderError('borderError')\r\n            }\r\n        }\r\n    }\r\n\r\n    // hook voir le password\r\n    const seePwd = (e) => (\r\n        seePassword === 'password' ?\r\n            dispatch({\r\n                type: 'SEE_